		"@sveltestrap/sveltestrap": "^6.2.7",
		"@types/google-protobuf": "^3.15.12",
		"axios": "^1.7.2",
		"buffer": "^6.0.3",
		"capacitor-bluetooth": "file:../capacitor-bluetooth",
		"fuse.js": "^7.0.0",
		"google-protobuf": "^3.21.2",
		"nats.ws": "^1.29.2",
		"rpc-websockets": "^7.11.2"
	}
